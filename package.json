{
  "name": "postcss-styled-components",
  "version": "0.0.1",
  "description": "PostCSS syntax for parsing styled-components",
  "keywords": [
    "postcss",
    "syntax",
    "styled-components",
    "css-in-js",
    "css"
  ],
  "repository": "sangotaro/postcss-styled-components",
  "license": "MIT",
  "author": "sangotaro",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "fmt": "prettier . --write",
    "lint": "npm-run-all --parallel lint:*",
    "lint:fmt": "prettier . --check",
    "lint:js": "eslint . --cache --max-warnings=1",
    "lint:md": "remark . --quiet --frail",
    "release": "np",
    "test": "jest",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@babel/core": ">=7.15.5",
    "postcss": "8.3.11"
  },
  "devDependencies": {
    "@stylelint/remark-preset": "3.0.0",
    "@types/jest": "27.0.2",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "codecov": "3.8.3",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-regexp": "1.5.0",
    "jest": "27.3.1",
    "json5": "2.2.0",
    "np": "7.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "remark-cli": "10.0.0",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  }
}
