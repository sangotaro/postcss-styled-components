{
  "name": "postcss-styled-components",
  "version": "0.0.1",
  "description": "PostCSS syntax for parsing styled-components",
  "keywords": [
    "postcss",
    "syntax",
    "styled-components",
    "css-in-js",
    "css"
  ],
  "repository": "sangotaro/postcss-styled-components",
  "license": "MIT",
  "author": "sangotaro",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "fmt": "prettier . --write",
    "lint": "npm-run-all --parallel lint:*",
    "lint:fmt": "prettier . --check",
    "lint:js": "eslint . --cache --max-warnings=0",
    "lint:md": "remark . --quiet --frail",
    "release": "np",
    "test": "jest",
    "watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!coverage/**",
      "!test{,s}/**",
      "!**/.{prettier,eslint}rc.{js,cjs}"
    ],
    "testMatch": [
      "**/test/*.js"
    ]
  },
  "dependencies": {
    "@babel/core": ">=7.15.5",
    "postcss": "8.3.11",
    "postcss-safe-parser": "6.0.0"
  },
  "devDependencies": {
    "@stylelint/remark-preset": "3.0.0",
    "autoprefixer": "9.8.8",
    "codecov": "3.8.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-stylelint": "13.1.0",
    "jest": "27.3.1",
    "json5": "2.2.0",
    "np": "7.5.0",
    "npm-run-all": "4.1.5",
    "postcss-parser-tests": "6.5.0",
    "prettier": "2.4.1",
    "remark-cli": "10.0.0"
  },
  "peerDependencies": {
    "postcss": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
